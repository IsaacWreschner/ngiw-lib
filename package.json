{
  "name": "ngiw-lib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"npm:watch-lib\" \"npm:watch-showcase-app\" \"npm:storybook\"",
    "prod": "ng build ngiw-common && ng build showcase-app",
    "watch-lib": "ng build ngiw-common --watch --configuration development",
    "watch-showcase-app": "ng serve --configuration development",
    "storybook": "ng run ngiw-common:storybook",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.1.6",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.1.6",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "marked": "^11.0.0",
    "ng-zorro-antd": "16.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.6",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "^16.1.0",
    "@chromatic-com/storybook": "^1.4.0",
    "@compodoc/compodoc": "^1.1.24",
    "@storybook/addon-docs": "^8.1.3",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/angular": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.0.0",
    "storybook": "^8.1.3",
    "typescript": "~5.1.3"
