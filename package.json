{
  "name": "ngiw-lib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"npm:watch-lib\" \"npm:watch-showcase-app\" \"npm:storybook\"",
    "prod": "npm run lint-lib &&  npm run build-lib && npm run lint-showcase-app && npm run test-showcase-app && npm run build-showcase-app",
    "watch-lib": "ng build ngiw-common --watch --configuration development",
    "watch-showcase-app": "ng serve --configuration development",
    "build-lib": "ng build ngiw-common --configuration production",
    "build-showcase-app": "ng build showcase-app --configuration production",
    "lint-lib": "ng lint ngiw-common",
    "lint-showcase-app": "ng lint showcase-app",
    "test-lib": "ng test ngiw-common --watch=false",
    "test-showcase-app": "ng test showcase-app --watch=false",
    "storybook": "storybook dev",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.10",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.10",
    "@angular/compiler": "^17.3.10",
    "@angular/core": "^17.3.10",
    "@angular/forms": "^17.3.10",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.10",
    "@angular/platform-browser-dynamic": "^17.3.10",
    "@angular/router": "^17.3.10",
    "marked": "^11.0.0",
    "ng-zorro-antd": "17.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/builder": "17.5.2",
    "@angular-eslint/eslint-plugin": "17.5.2",
    "@angular-eslint/eslint-plugin-template": "17.5.2",
    "@angular-eslint/schematics": "17.5.2",
    "@angular-eslint/template-parser": "17.5.2",
    "@angular/cli": "~17.3.8",
    "@angular/compiler-cli": "^17.3.10",
    "@chromatic-com/storybook": "^1.4.0",
    "@compodoc/compodoc": "^1.1.24",
    "@storybook/addon-docs": "^8.1.3",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/angular": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "storybook": "^8.1.3",
    "typescript": "5.4.5"
  }
}