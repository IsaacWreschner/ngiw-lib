{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "core-ui": {
      "projectType": "library",
      "root": "projects/core-ui",
      "sourceRoot": "projects/core-ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/core-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/core-ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/core-ui/tsconfig.lib.json"             
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/core-ui/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ngiw-core-utils": {
      "projectType": "library",
      "root": "projects/ngiw-core-utils",
      "sourceRoot": "projects/ngiw-core-utils/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngiw-core-utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngiw-core-utils/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngiw-core-utils/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngiw-core-utils/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "c99d7d0b-3bcf-4c2b-805a-8344efd05655"
  }
}
