{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngiw-core-ui": {
      "projectType": "library",
      "root": "projects/ngiw-core-ui",
      "sourceRoot": "projects/ngiw-core-ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngiw-core-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngiw-core-ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngiw-core-ui/tsconfig.lib.json"             
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngiw-ngiw-core-ui/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ngiw-core-utils": {
      "projectType": "library",
      "root": "projects/ngiw-core-utils",
      "sourceRoot": "projects/ngiw-core-utils/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngiw-core-utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngiw-core-utils/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngiw-core-utils/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngiw-core-utils/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "showcase-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/showcase-app",
      "sourceRoot": "projects/showcase-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/showcase-app",
            "index": "projects/showcase-app/src/index.html",
            "main": "projects/showcase-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/showcase-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/showcase-app/src/favicon.ico",
              "projects/showcase-app/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "projects/showcase-app/src/assets/"
              }
            ],
            "styles": [
              "projects/showcase-app/src/styles.scss",
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css"

            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "showcase-app:build:production"
            },
            "development": {
              "browserTarget": "showcase-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "showcase-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/showcase-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/showcase-app/src/favicon.ico",
              "projects/showcase-app/src/assets"
            ],
            "styles": [
              "projects/showcase-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "c99d7d0b-3bcf-4c2b-805a-8344efd05655"
  }
}
